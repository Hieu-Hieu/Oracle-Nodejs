<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>
    roles
  </title>
  <link rel="shortcut icon" href="./images/logo-mb.png" type="image/png">
  <!-- GOOGLE FONT -->
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link
    href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
    rel="stylesheet">
  <!-- BOXICONS -->
  <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
  <!-- APP CSS -->
  <link rel='stylesheet' href='/stylesheets/grid.css'>
  <link rel="stylesheet" href="/stylesheets/app.css">
</head>

<body>

  <!-- SIDEBAR -->
  <div class="sidebar">
    <div class="sidebar-logo">
      <img src="./images/logo-lg.png" alt="Comapny logo">
      <div class="sidebar-close" id="sidebar-close">
        <i class='bx bx-left-arrow-alt'></i>
      </div>
    </div>
    <div class="sidebar-user">
      <div class="sidebar-user-info">
        <img src="./images/user-image-2.png" alt="User picture" class="profile-image">
        <div class="sidebar-user-name">
          anh tuat
        </div>
      </div>
      <button class="btn btn-outline">
        <i class='bx bx-log-out bx-flip-horizontal'></i>
      </button>
    </div>
    <!-- SIDEBAR MENU -->
    <ul class="sidebar-menu">
      <li>
        <a href="./index.html" class="active">
          <i class='bx bx-home'></i>
          <span>dashboard</span>
        </a>
      </li>
      <li>
        <a href="./user.html">
          <i class='bx bx-user-circle'></i>
          <span>Quản lí user</span>
        </a>
      </li>
      <li>
        <a href="./profile.html">
          <i class='bx bx-user-circle'></i>
          <span>Quản lí profile</span>
        </a>
      </li>
      <li>
        <a href="./role.html">
          <i class='bx bx-user-circle'></i>
          <span>Quản lí role</span>
        </a>
      </li>
    </ul>
    <!-- END SIDEBAR MENU -->
  </div>
  <!-- END SIDEBAR -->

  <!-- MAIN CONTENT -->
  <div class="main">
    <div class="main-header">
      <div class="mobile-toggle" id="mobile-toggle">
        <i class='bx bx-menu-alt-right'></i>
      </div>
      <div class="main-title">
        dashboard
      </div>
    </div>
    <div class="main-content">
      <div class="row">
        <div class="col-12">
          <!-- ORDERS TABLE -->
          <div class="box">
            <div class="box-header">
              Roles
            </div>

            <div class="box-body overflow-scroll height-300">
              <table>
                <thead>
                  <tr>
                    <th>ROLE</th>
                    <th>ROLE ID</th>
                    <th>PASSWORD REQUIRED</th>
                    <th>AUTHENTICATION TYPE</th>
                    <th>COMMON</th>
                    <th>ORACLE MAINTAINED</th>
                    <th>INHERITED</th>
                    <th>IMPLICIT</th>
                    <th>EXTERNAL_NAME</th>
                  </tr>
                </thead>
                <tbody>
                  <% if(roles !=null) { %>
                    <tr>
                      <% roles.forEach(function(quota, index) { %>

                        <td>
                          <%- quota.ROLE %>
                        </td>
                        <td>
                          <%- quota.ROLE_ID %>
                        </td>
                        <td>
                          <%- quota.PASSWORD_REQUIRED %>
                        </td>
                        <td>
                          <%- quota.AUTHENTICATION_TYPE %>
                        </td>
                        <td>
                          <%- quota.COMMON %>
                        </td>
                        <td>
                          <%- quota.ORACLE_MAINTAINED %>
                        </td>
                        <td>
                          <%- quota.INHERITED %>
                        </td>
                        <td>
                          <%- quota.IMPLICIT %>
                        </td>
                        <td>
                          <%- quota.EXTERNAL_NAME %>
                        </td>
                    </tr>

                    <% }) %>
                      <% } else { %>
                        <tr>

                          Không có dữ liệu
                        </tr>
                        <% } %>
                </tbody>
              </table>
            </div>

          </div>
        </div>
        <!-- END ORDERS TABLE -->
      </div>
    </div>
  </div>
  </div>
  <!-- END MAIN CONTENT -->

  <div class="overlay"></div>

  <!-- APP JS -->
  <script src="/javascript/app.js" type='text/javascript'></script>
</body>

</html>